    "arch": "x86_64",
    "image": "minimal",
    "release": "7",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
	    "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    }
  },
  "jessie_amd64": {
    "distro": "debian",
