{
  "example_1404_amd64": {
    "distro": "ubuntu",
    "description": "ubuntu LTS 1404 amd64 vagrant build",
    "arch": "amd64",
    "image": "server",
    "release": "14.04",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "memory=4096"
          ]
        }
      }
    }
  },
  "centos7_x86_64": {
    "distro": "centos",
