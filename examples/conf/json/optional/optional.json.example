{
  "chef-solo": {
    "distro": "ubuntu",
    "description": "ubuntu LTS 1404 amd64 server build using chef-solo provisioner",
    "arch": "amd64",
    "image": "server",
    "release": "14.04",
    "builder_types": [
      "virtualbox-iso"
    ],
    "post_processor_types": [
      "vagrant"
    ],
    "provisioner_types": [
      "chef-solo"
    ],	
    "builders": {
      "common": {
        "settings": [
           "ssh_wait_timeout = 300m"
        ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
             "memory=4096"
           ]
         }
       }
    }
  }
}	
