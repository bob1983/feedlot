{
  "1404-64": {
    "distro": "ubuntu",
    "description": "example Ubuntu LTS 1404 amd64 vagrant build",
    "arch": "amd64",
    "image": "server",
    "release": "14.04",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "memory=4096"
          ]
        }
      }
    }
  },
  "centos7-64": {
    "distro": "centos",
    "description": "example Centos 7 x86_64 build",
    "arch": "x86_64",
    "image": "minimal",
    "release": "7",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
	    "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    }
  },
  "jessie-64": {
    "distro": "debian",
    "description": "example Debian Jessie build",
    "arch": "amd64",
    "image": "netinst",
    "release": "8",
    "builder_types": [
      "virtualbox-iso"
    ],
	"builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
         ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    }
  }
}
