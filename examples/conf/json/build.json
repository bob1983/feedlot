{
  "example_1404_amd64": {
    "distro": "ubuntu",
    "description": "example ubuntu LTS 1404 amd64 vagrant build",
    "arch": "amd64",
    "image": "server",
    "release": "14.04",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "memory=4096"
          ]
        }
      }
    }
  },
  "centos7_x86_64": {
    "distro": "centos",
    "description": "example centos 7 x86_64 build",
    "arch": "x86_64",
    "image": "minimal",
    "release": "7",
    "builder_types": [
      "virtualbox-iso"
    ],
    "builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
        ]
      },
      "virtualbox-iso": {
	    "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    }
  },
  "jessie_amd64": {
    "distro": "debian",
    "description": "example debian jessie build",
    "arch": "amd64",
    "image": "netinst",
    "release": "8",
    "builder_types": [
      "virtualbox-iso"
    ],
	"builders": {
      "common": {
        "settings": [
          "ssh_wait_timeout = 15m"
         ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    }
  }
}
