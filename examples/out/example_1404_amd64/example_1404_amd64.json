{
	"description": "ubuntu LTS 1404 amd64 vagrant build",
	"min_packer_version": "0.7.0",
	"builders": [
		{
			"boot_wait": "5s",
			"disk_size": 20000,
			"guest_additions_path": "VBoxGuestAdditions_{{ .Version }}.iso",
			"guest_os_type": "Ubuntu_64",
			"headless": true,
			"http_directory": "virtualbox-iso/http",
			"iso_checksum": "8acd2f56bfcba2f7ac74a7e4a5e565ce68c024c38525c0285573e41c86ae90c0",
			"iso_checksum_type": "sha256",
			"iso_url": "http://releases.ubuntu.com/14.04/ubuntu-14.04.2-server-amd64.iso",
			"output_directory": ":out_dir",
			"shutdown_command": "shutdown.command.example",
			"ssh_password": "packer",
			"ssh_port": 22,
			"ssh_username": "packer",
			"ssh_wait_timeout": "15m",
			"type": "virtualbox-iso",
			"vboxmanage": [
				[
					"modifyvm",
					"{{.Name}}",
					"--memory",
					"4096"
				]
			],
			"virtualbox_version_file": ".vbox_version",
			"vm_name": "example_1404_amd64"
		}
	],
	"post-processors": [
		{
			"compression_level": 1,
			"output": "proxycore_{{.Provider}}.box",
			"type": "vagrant",
			"vagrantfile_template": "vagrantfile.tpl"
		}
	],
	"provisioners": [
		{
			"scripts": [
				"shell/setup.sh",
				"shell/sudoers.sh",
				"shell/packer.sh",
				"shell/cleanup.sh"
			],
			"type": "shell"
		}
	]
}