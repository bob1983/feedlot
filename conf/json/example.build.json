{
  "1404-amd64": {
    "distro": "ubuntu",
    "description": "ubuntu LTS 1404 amd64 server build, minimal install",
    "arch": "amd64",
    "image": "server",
    "release": "14.04",
    "builder_types": [
      "virtualbox-iso"
    ],
    "post_processor_types": [
      "vagrant"
    ],
    "provisioner_types": [
      "shell"
    ],	
    "builders": {
      "common": {
        "settings": [
           "ssh_wait_timeout = 300m"
        ]
      },
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
             "memory=4096"
           ]
         }
       }
    }
  },
  "centos7_x86_64": {
    "distro": "centos",
    "description": "Centos 7 x86_64 w virtualbox-iso only",
    "arch": "x86_64",
    "release": "7",
    "builder_types": [
      "virtualbox-iso"
    ],
    "post_processor_types": [
      "vagrant"
    ],
    "provisioner_types": [
      "shell"
    ]
  },
  "jessie_amd64": {
    "distro": "debian",
    "description": "debian jessie amd64",
    "arch": "amd64",
    "release": "8",
    "builder_types": [
      "virtualbox-iso"
    ],
    "post_processor_types": [
      "vagrant"
    ],
    "provisioner_types": [
      "shell"
    ],
    "builders": {
      "virtualbox-iso": {
        "arrays": {
          "vboxmanage": [
            "--memory=4096"
          ]
        }
      }
    },
    "provisioners": {
      "shell": {
        "arrays": {
           "scripts": [ 
              "setup.sh",
              "sudoers.sh",
              "vagrant.sh",
              "customize.sh",
              "cleanup.sh"
            ]
         }
      }
    }
  }
}	
