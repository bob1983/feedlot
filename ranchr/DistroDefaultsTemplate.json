{PackerInf:{
	MinPackerVersion: Description:
} 
IODirInf:{
	OutDir:out/ubuntu/:date SrcDir:src/ubuntu CommandsDir: ScriptsDir:
} 
date: 
BaseURL:http://releases.ubuntu.com 
delim: 
Type:ubuntu 
name: 
arch: 
image: 
version: 
varVals:map[] 
vars:map[] 
build:{
	BuilderType:[virtualbox-iso vmware-iso] 
	Builders:map[
		common:{
			Settings:[
				boot_command = :commands_dir/boot.command boot_wait = 5s 
				disk_size = 20000 http_directory = http 
				iso_checksum_type = sha256 shutdown_command = :commands_dir/shutdown.command 
				ssh_password = vagrant ssh_port = 22 
				ssh_username = vagrant ssh_wait_timeout = 240m
			] 
			VMSettings:[]} 
		virtualbox-iso:{
			Settings:[] 
			VMSettings:[cpus=1 memory=2048]
		} 
		vmware-iso:{
			Settings:[] 
			VMSettings:[cpuid.coresPerSocket=1 memsize=1024 numvcpus=1 memory=2048]
		}
	] 
	PostProcessors:map[
		vagrant:{
			Settings:[keep_input_artifact = false output = :out_dir/someComposedBoxName.box]
		}
	] 
	Provisioners:map[
		shell:{
			Settings:[execute_command = :commands_dir/execute.command] 
			Scripts:[:scripts_dir/setup.sh :scripts_dir/base.sh :scripts_dir/vagrant.sh :scripts_dir/cleanup.sh :scripts_dir/zerodisk.sh]
		}
	]
}}
